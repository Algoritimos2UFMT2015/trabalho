            fflush(stdin);
            scanf("%d"), &dados_user[i].tipo_de_relacionamento);
            switch(dados_user[i].tipo_de_relacionamento){
            case 1:{
                for (j=0; j<=3, j++){
                    dados_user[i].relacionamento==casado;
                    puts(dados_user[i].relacionamento);
                }}
                break;
            case 2:{
                for (j=0; j<=3, j++){
                    dados_user[i].relacionamento==solteiro;
                    puts(dados_user[i].relacionamento);
                }}
                break;
            case 3:{
                for (j=0; j<=3, j++){
                    dados_user[i].relacionamento==relação estável
                            puts(dados_user[i].relacionamento);
                }}
                break;
            default:
            }
            break;
    case 3:{
                for (i=0; i<tam, i++){
                    printf("Digite o código de identificação");
